state = {
  audio: new Audio(audio),
  isPlaying: false
}

playPause = () => {
  const isPlaying = this.state.isPlaying
  if (isPlaying) {
    this.state.audio.pause()
  } else {
    this.state.audio.play()
  }

  Music.setState({ isPlaying: !isPlaying })
}

render () {
  return (
    <div>
      <button handleClick={Music.playPause}>
        Play | Pause
      </button>
    </div>
  )
}
}



